{
  "parser": "babel-eslint",
  "env": {
    "browser": true,
    "es6": true,
    "jest": true
  },
  "settings": {
    "react": {
      "version": "detect"
    },
    "import/resolver": {
      "node": {
        "extensions": [".js", ".jsx", ".ts", ".tsx", ".json"]
      }
    }
  },
  "extends": [
    "eslint:recommended",
    "plugin:import/errors",
    "plugin:react/recommended",
    "plugin:jsx-a11y/recommended",
    "prettier",
    "prettier/react"
  ],
  "globals": {
    "process": true,
    "Atomics": "readonly",
    "SharedArrayBuffer": "readonly",
    "fetch": true,
    "importScripts": true,
    "self": true,
    "cy": true,
    "context": true
  },
  "parserOptions": {
    "ecmaVersion": 2018,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "rules": {
    "no-unused-vars": "error",
    "react/prop-types": 0,
    "react-hooks/rules-of-hooks": "error",
    "no-console": "off"
  },
  "plugins": ["react", "import", "jsx-a11y", "react-hooks"],
  "root": true
}
